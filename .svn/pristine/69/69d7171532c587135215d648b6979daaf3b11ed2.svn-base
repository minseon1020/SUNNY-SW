.wrap { padding: 20px; }
.header h1 { font-size: 26px; margin: 4px 0 8px; }
.header p { color: #6b7280; margin: 0 0 16px; }

.card {
  background: #fff;
  border: 1px solid #e5e7eb;
  border-radius: 16px;
  padding: 14px;
  box-shadow: 0 2px 10px rgba(0,0,0,.04);
}
.cardHead { display: flex; align-items: center; justify-content: space-between; margin-bottom: 8px; }
.panel { height: 600px; padding: 0; }
